.icon-shoucang:before { content: "\e627"; }

.icon-wode:before { content: "\e62f"; }

.icon-shouye:before { content: "\e62e"; }

.icon-xiaoxi:before { content: "\e630"; }

.icon-dingyue:before { content: "\e63f"; }

